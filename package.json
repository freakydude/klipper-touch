    "dotenv": "^16.0.3",
    "postcss": "^8.4.21",
    "svelte": "^3.58.0",
    "svelte-fa": "^3.0.3",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1"
  }
