{
  "name": "klipper-touch",
  "version": "0.1.0",
  "description": "Klipper-Touch is an alternative, web-based user interface for Klipper3D/Moonraker, designed for use with small touch screens that do not have a keyboard or mouse.",
  "author": "Frank Roth <developer@freakydu.de>",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "lint": "prettier --write . --check . && eslint .",
    "format": "prettier --write ."
  },
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/vite": "~4.1.18",
    "@tauri-apps/api": "2.10.1",
